<?xml version="1.0"?>
<package>
  <metadata>
    <id>OpenSilver.FloatableWindow</id>
    <version>$PackageVersion$</version>
    <authors>Userware</authors>
    <projectUrl>https://www.opensilver.net</projectUrl>
    <repository type="git" url="https://github.com/OpenSilver/FloatableWindow" />
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This is an adaptation of the ChildWindow control for Silverlight ported to OpenSilver.</description>
    <copyright>Copyright (c) 2023, Userware. All rights reserved.</copyright>
    <tags>silverlight webassembly csharp xaml wasm dotnet .net C# blazor</tags>
    <icon>images\OpenSilver_icon.png</icon>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="OpenSilver" version="$OpenSilverDependencyVersion$" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- OpenSilver runtime -->
    <file src="../FloatableWindow/bin/$Configuration$/netstandard2.0/FloatableWindow.dll" target="lib/netstandard2.0/FloatableWindow.dll" />
    <file src="../FloatableWindow/bin/$Configuration$/netstandard2.0/FloatableWindow.pdb" target="lib/netstandard2.0/FloatableWindow.pdb" />

    <!-- OpenSilver icon -->
    <file src="OpenSilver_icon.png" target="images\OpenSilver_icon.png" />
  </files>
</package>
